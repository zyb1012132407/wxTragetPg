/* pages/CssTest/CssTest.wxss */
.title{
  margin:50px,20px,20px,20px; 
  /* font-family:monospace; */
  font-size: 50px;
  color: white;
}
.targetSetCard{
  width: 700rpx;
  height:200rpx;
  border-radius: 20rpx;
  margin-top: 20px;
  position: relative;
  left: 3%;
  padding: 0;
  box-shadow: 4px 4px 10px #B721FF;
}

.navigator{
  position: relative;
  width: 100%;
  height: 100%;
}


.card1color{
  background-image: linear-gradient(120deg, #d4fc79 0%, #96e6a1 100%);  box-shadow: 4px 4px 10px #96e6a1;
}

.card2color{
background-color: #FFE53B;
background-image: -webkit-linear-gradient(147deg, #FFE53B 0%, #FF2525 74%);
background-image: -moz-linear-gradient(147deg, #FFE53B 0%, #FF2525 74%);
background-image: -o-linear-gradient(147deg, #FFE53B 0%, #FF2525 74%);
background-image: linear-gradient(147deg, #FFE53B 0%, #FF2525 74%);
box-shadow: 4px 4px 10px #FF2525;
}

.card3color{
background-color: #FEE140;
background-image: -webkit-linear-gradient(212deg, #FEE140 0%, #f3fa70 100%);
background-image: -moz-linear-gradient(212deg, #FEE140 0%, #f3fa70 100%);
background-image: -o-linear-gradient(212deg, #FEE140 0%, #f3fa70 100%);
background-image: linear-gradient(212deg, #FEE140 0%, #f3fa70 100%);

  box-shadow: 4px 4px 10px #FEE140;
}

.card4color{
  background-image: linear-gradient(-225deg, #9EFBD3 0%, #57E9F2 48%, #45D4FB 100%);box-shadow: 4px 4px 10px #57E9F2;
}

.card5color{
  background-image: linear-gradient(to right, #43e97b 0%, #38f9d7 100%);
  box-shadow: 4px 4px 10px #38f9d7;
}

@keyframes bounce {
  from,
  20%,
  53%,
  80%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
    transform: translate3d(0, 0, 0);
  }

  40%,
  43% {
    animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    transform: translate3d(0, -30px, 0);
  }

  70% {
    animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    transform: translate3d(0, -15px, 0);
  }

  90% {
    transform: translate3d(0, -4px, 0);
  }
}

.bounce {
  animation-name: bounce;
  transform-origin: center bottom;
}



