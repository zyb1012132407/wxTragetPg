.title{
  font-size: 60rpx;
  font-weight: bold;
  padding-bottom: 20rpx;
}
.target{
  width: 700rpx;
  background-color: #ffffff;
  height:150rpx;
  border: 2px solid #EEEEEE;
  border-radius: 5rpx;
  box-shadow: 4px 1px 1px #EEEEEE;
  margin: 5px;
  position: relative;
  padding: 0
}
.targetIco{
  display:flex;
  align-items:center;
  justify-content:center;
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  position: absolute;
  margin: 0;
  left: 83%;
  top:10%;
}
.targetAddIco{
  width: 100rpx;
  height: 100rpx;
  top:18%;
  left: 44%;
  position: absolute;
  margin: 0;
}
.targetText{
  width: 500rpx;
  height: 100rpx;
  position: absolute;
  top: 15%;
  left: 10%;
}
.targetProgress{
  position: relative;
  top: 90%;
}
/*遮罩层  */    
.shade{    
  width:100%;    
  height:100%;    
  top:0;    
  background:rgba(0,0,0,0.5);    
  overflow: hidden;    
  z-index: 1;     
  position: absolute;     
} 
/* 遮罩内容 */
.conts{    
  width: 100%;    
  height: 428rpx;    
  background-color: white;    
  position: fixed;    
  top: 650rpx;     
  z-index: 2;
  padding-top: 50rpx;   
} 
.conts .font{
  border: 1px solid blue;
  border-radius: 20rpx;
  width: 600rpx;
  height: 80rpx;
  margin: 20rpx;
  position: relative;
  left:9%;
  align-items: center;
}
.conts .button01{
  width: 300rpx;
  height: 80rpx;
  border-radius: 20rpx;
  padding: 0;
  position: fixed;
  left:30%;
  display: table;
}
.conts .button01 text {
  vertical-align:middle;
  display: table-cell;
}  
